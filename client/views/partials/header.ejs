                <% }else{ %>
                    <a href="#" class="btn btn-outline-primary ml-3 mr-3">Hi <%=info.nickname%></a>
                    <a href="/signout" class="btn btn-outline-danger ml-3 mr-3">Sign Out</a>
                <% } %>
            </div>
        </nav>
